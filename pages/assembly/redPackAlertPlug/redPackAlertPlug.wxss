
.redPackAlertPlug{
  position: absolute;
  z-index:10000;
  width:300px;
  height:400px;
  background:url('http://on3s1z2us.bkt.clouddn.com/WechatIMG109.jpeg');
  background-size:105% 105%;
  background-position: 0px -5px;
  left:50%;
  margin-left:-150px;
  top:10vh;
  border-radius:10px;
}

.redPackAlertPlugClose{
  position: absolute;
  right:20px;
  color:rgba(170,70,50,1);
  z-index:10000;
  width:50px;
  text-align: right;
  padding-top:30px;
  padding-bottom:30px;

  top:-20px;
}

.redPackAlertPlugList{
  padding-top:150px;

  padding-left:50px;

  position: relative;
}

.redPackAlertPlugItem{
  color:rgba(235,205,154,1);
  font-size:15px;
}

.redPackAlertPlugButton{
  background:rgba(235,205,154,1);
  text-align: center;
  width:70px;
  height:70px;
  border-radius:100px;
  line-height: 70px;
  font-size:20px;
  position: absolute;

  top:300px;

  left:50%;

  margin-left:-35px;
}

.redPackAlertPlugImage{
  width:90px;
  height:90px;
  position: absolute;
  top:10px;
  left:50%;
  margin-left:-45px;
  border-radius:90px;
  background:RGBA(254,0,54,1);

}

.redPackAlertPlugNickname{
  color:rgba(235,205,154,1);
  width:100px;
  left:50%;
  margin-left:-50px;
  font-size:15px;
  position: absolute;
