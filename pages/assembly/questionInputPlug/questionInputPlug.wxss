.inputItem{
	width:100%;
	height:70px;
	
	margin-top: 5px;

  text-align:center;

}

.inputItemInput{
	width:75%;
  padding-top:18px;
	height:40px;
	
	font-size:25px;
	
	border: 0px solid white;
	
	background-color: white;
	
	display: inline-block;
	
	padding-left: 20px;
	
	border-radius:10px;
}

.inputItemButton{
  width:80%;
  background-color:green;
  margin: 0 auto;
  border-radius:10px;
  padding-top:7px;
  padding-bottom:7px;
  font-size:20px;
  color:black;
  margin-top:10px;
}


.inputItemFillTerms{
	padding-top: 10px;
  width:100%;

  text-align: center;
}

.inputItemFillTermsWorlds{
	width:100%;
  height:50px;
  text-align: center;
}

.inputItemFillTermsWorld{
	background-color: black;
	color: white;
	
	border:1px solid RGBA(215,215,217,1);
	
	width:14%;
	
	height:50px;
	
	padding-top:2px;
	
	vertical-align:top;
	
	font-size:30px;
	
	font-weight:bolder;
	
	display: inline-block;
	
	border-radius:4px;
}

.inputItemFillTermsWorldsCheck{
  width:100%;
  text-align: center;
}

.inputItemFillTermsWorldCheckItem{
	background-color: white;
	color: black;
	
	border:1px solid RGBA(215,215,217,1);
	
	width:40px;
	
	height:40px;
	
	margin-left:5px;
	
	margin-right:5px;
	
	font-size:20px;
	
	font-weight:bolder;
	
	display: inline-block;
	
	border-radius:4px;
	
	margin-top: 10px;
	
	vertical-align: top;

  line-height:40px;
}

.subjectOptions{
  width:100%;
}

.subjectOption{
	background-color: white;
	width:90%;
	height: 40px;
	border-radius:10px;
	margin: 0px auto;
	text-align: center;
	color: black;
	margin-top: 10px;
	font-size: 20px;
  padding-top:10px;
}