.battleQuickRoom{
  padding-top:40px;
  height: 95vh;
  position: absolute;
  z-index: 1000000;
}

.battleQuickRoomItems{
  height: 100%;
}

.battleQuickRoomItem{
  margin-bottom: 10px;
  padding-bottom: 20px;
  border-bottom: 1px solid rgba(117,117,117,0.3);
  position: relative;
}
.battleQuickRoomItemHead{
  padding-top:10px;
  padding-left:10px;
}

.battleQuickRoomItemHeadButton{
  display: inline-block;
  position: absolute;
  right: 10px;
  background: rgba(247,247,247,1);
  color: rgba(117,117,117,1);
  padding-top:10px;
  padding-bottom:10px;
  padding-left:20px;
  padding-right:20px;
  border-radius: 5px;
  top:30px;
}
.battleQuickRoomItemHeadImg{
  width:100px;
  height: 100px;
  border-radius: 10px;
  display: inline-block;
}

.battleQuickRoomItemHeadCotent{
  display: inline-block;
  vertical-align: top;
}
.battleQuickRoomItemHeadCotentName{
  color: black;
  font-size: 20px;
  padding-left: 20px;
  padding-top:10px;
}

.battleQuickRoomItemHeadCotentStatus{

  color: red;
  padding-left:20px;

  padding-top:10px;
}

.battleQuickRoomItemDetail{
  /*color: rgba(159,159,159,1);*/
  color:black;
  font-size:15px;
  padding-left:10px;
  padding-right: 10px;
}

.battleQuickRoomItemImgs{
  width:100%;
  white-space:nowrap;
}

.battleQuickRoomItemImg{
  width:100px;
  height:100px;
}

.battleQuickRoomItemRanks{
 
}

.battleQuickRoomItemRank{
  position: relative;
  width:50px;
  height: 50px;
  display: inline-block;
  margin: 5px;
}

.battleQuickRoomItemRankButton{
  text-align: right;
  color: rgba(117,117,117,1);
  position: relative;
  right: 10px;
}

.battleQuickRoomItemRankNum{
  position: absolute;
  z-index: 2;
  
}

.battleQuickRoomItemRankImg{
  width:50px;
  height: 50px;
  position: absolute;
  border-radius: 50px;
  z-index: 1;
}