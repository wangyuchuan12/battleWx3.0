.rankBattle{
  width:100%;
  background:rgba(234,231,228,1);

  padding-top:40px;
  height: 95vh;
  position: absolute;
  z-index: 1000000;
}
.rankBattleImg{
  width:100%;
  height:40vh;
  position: relative;
  margin: 0 auto;
}

.rankBattleRanks{
  height:40vh;
  padding-bottom:50px;
}

.rankBattleRank{
  width:100%;
  background:white;
  padding-bottom:10px;
  padding-top:10px;
  margin-top: 1px;
  position: relative;
}

.rankBattleRankImg{
  width:35px;
  height:35px;

  border-radius:100px;

  display: inline-block;

  position: relative;

  left:10px;
}

.rankBattleRankName{
  display: inline-block;
  font-size:15px;
  color: black;
  font-weight: bolder;
  vertical-align: top;
  position: relative;
  left:10px;
  top:7px;
}

.rankBattleRankNum{
  display: inline-block;
  padding-left:20px;
  top:-10px;
  position: relative;
  width:10px;
}

.rankBattleButtons{
  position: fixed;
  bottom:0px;
  width:100%;
  background:rgba(68,82,163,0.2);
  padding-top:10px;
  padding-bottom:10px;
  text-align: center;
}

.rankBattleButton{
  width:45%;
  background:green;
  color: white;
  display: inline-block;
  border-radius: 5px;
  margin-left:1%;
  text-align: center;
}

.rankBattleCoverLabel{
  display: inline-block;
  font-size:10px;
  color: red;
  position: absolute;
  top:40px;
  left:80px;
}

.firstRank{
  position: absolute;
  top:10px;
  z-index: 1000;
  text-align: center;
  left:10px;

}

.firstRankImg{
  display: inline-block;
  width:50px;
  height:50px;
  border-radius: 50px;
}

.firstRanknickname{
  display: inline-block;
  font-size:15px;
  color: white;
  background:rgba(54,198,239,0.2);
  vertical-align: top;
  left:10px;
  top:10px;
  position: relative;
}

.rankBattleRankScore{
  position: absolute;
  right:20px;
  top:15px;
  width:80px;
}

.rankBattleTabList{
  text-align: center;
  padding-bottom:5px;
}

.rankBattleTabItem{
  display: inline-block;
  width:49%;
  text-align: center;

  padding-top:10px;
  padding-bottom:10px;
}