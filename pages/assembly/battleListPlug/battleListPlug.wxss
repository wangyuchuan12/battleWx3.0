
.battleList{
  height: 100vh;
  width: 100%;
  background: rgba(49,64,93,1);
}

.battleItem{
  width:45%;
  height:150px;
  display:inline-block;
}
.battleItemImg{
  width:100%;
  height:100%;
}



.battleHomeContentItem2{
  width:100px;
  height: 70px;
  border-radius:10px;
  position: relative;
  left:10px;
  display:inline-block;
  margin-top:10px;
}

.battleHomeContentItem2Name{
  font-size:20px;
  color: white;
  display: inline-block;
  vertical-align: top;
  left:20px;
  top:20px;
  position: relative;

  width:40%;

  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.battleHomeContentItem2NameFooter{
  color: white;
  display: block;
  float: right;
  top:20px;
  position: relative;
  right:20px;
  font-size:40px;
}

.battleHomeContentItem2Tool{
  width:100%;
  text-align: center;

  padding-top:2px;

  margin-bottom:20px;
}

.battleHomeContentItem2ToolItem{
  display:inline-block;
  width:31%;
  color: white;
  font-size:30px;
  background:rgba(38,75,140,0.5);
  border-radius:5px;
  margin: 0.7%;
  vertical-align: top;
}

.battleHome{
  width:100%;
  height:100vh;
  padding-top:40px;
  background: rgba(49,64,93,1);
  position: fixed;
}

.battleHomeTool{
  text-align:right;
  padding-top:10px;
  padding-bottom:10px;
  padding-right:20px;
  color:white;
  
}

.battleHomeToolItem{
  display:inline-block;
  width:25%;
  text-align: center;
  margin-bottom:10px;
}

.battleHomeHeader{
  padding-top:10px;
  padding-bottom:10px;
  width:97%;
  background:rgba(38,75,140,0.5);
  margin-top:5px;
  margin: 0 auto;
  border-radius:5px;
}

.battleHomeHeaderImg{
  width:50px;
  height:50px;
  display:inline-block;
  left:10px;
  border-radius:10px;
  position: relative;
}

.battleHomeHeaderSelect{
  display:inline-block;
  width:40%;
  background:bule;
  color:white;
  vertical-align: top;
  text-align: center;
  left:20px;
  top:5px;
  position:relative;
  border-radius: 10px;
  padding-right:20px;
  border:2px solid RGBA(247,247,247,0.5);

  padding-top:5px;
  padding-bottom:5px;
  
}

.battleHomeHeaderSelectSpan1{
  display:inline-block;
  width:80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.battleHomeHeaderStart{
  display:block;
  position:relative;
  background: RGBA(245,173,56,1);
  border-radius:10px;
  color:white;
  float: right;
  padding-left:10px;
  padding-right:10px;
  padding-top:5px;
  padding-bottom:5px;
  right:10px;
  top:10px;
}

.battleHomeContentItem{
  background:rgba(38,75,140,0.5);
  width:100%;
  position: relative;
  text-align: left;
  padding-top:10px;
  padding-bottom:10px;
  margin-bottom:5px;
}

.battleHomeContentItemImage{
  width:40%;
  height:90px;
  position: relative;
  margin: 0 auto;
  display:inline-block;
  border-radius:10px;
  left:20px;
}

.battleHomeContentItemSmallImage{
  position: absolute;
  top:40px;
  left:30px;
  width:30px;
  height:30px;
  z-index:100;
  border-radius:10px;
}

.battleHomeContentItemPersonal{
  display:inline-block;
  vertical-align: top;
  padding-left:10px;
  padding-top:5px;
  width:45%;
  text-align: left;
}

.battleHomeContentItemPersonalName{
  color:red;
  position: absolute;
  border-radius:5px;
  top:20px;
  left:50%;
}

.battleHomeContentItemPersonalInstruction{
  font-size:10px;
  width:90%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  
}

.battleHomeContentItemFooter{
  display:block;
  font-size:12px;
  left:50%;
  top:50px;
  position: absolute;
  color: white;
}

.battleHomeContentMyself{
  background:rgba(38,75,140,0.5);
  margin: 5px;
  padding-top:10px;
  padding-bottom:10px;
}

.battleHomeContentMyselfIcon{
  display:inline-block;
  color:green;
  position: relative;
  padding-left:10px;
  font-size:20px;
}

.battleHomeContentMyselfName{
  display:inline-block;
  position: relative;
  padding-left:50px;
  color: white;
}


.battleHomeContentMyselfIcon2{
  float: right;
  right:10px;
  position: relative;
  color: white;
}

.battleHomeContentList{
  background: rgba(49,64,93,1);
}

.battleHomeFastEntryItem{
  display: inline-block;
  text-align: center;
  color: #fff;
  text-shadow:1px 1px 1px #333;
  border-radius: 5px;
  position: relative;
  overflow: hidden;
  background:RGBA(0,118,184,0.5);
  margin: 10px;
  width:30%;
  padding-top:10px;
}

.battleHomeFastEntry{
  text-align: center;
}
